{
    "name": "dotcms-webcomponents",
    "version": "0.0.19",
    "description": "DotCMS UI Web Components",
    "repository": {
        "type": "git",
        "url": "https://github.com/dotcms/dotcms-webcomponents"
    },
    "main": "./dist/index.cjs.js",
    "module": "./dist/custom-elements/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/dotcms-webcomponents/dotcms-webcomponents.js",
    "files": [
        "dist/",
        "loader/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "build.watch": "stencil build --watch",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "storybook": "start-storybook -s dist -p 6006",
        "build-storybook": "build-storybook",
        "dev": "npm-run-all --parallel start storybook"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@stencil/core": "^2.3.0",
        "@stencil/sass": "^1.4.1",
        "@storybook/addon-actions": "^6.1.11",
        "@storybook/addon-knobs": "^6.1.11",
        "@storybook/addon-notes": "^5.3.21",
        "@storybook/html": "^6.1.11",
        "@types/jest": "26.0.20",
        "@types/puppeteer": "5.4.2",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^7.0.0",
        "jest": "26.6.3",
        "jest-cli": "26.6.3",
        "npm-run-all": "^4.1.5",
        "puppeteer": "5.5.0",
        "write-file-webpack-plugin": "^4.5.1"
    },
    "license": "MIT",
    "dependencies": {
        "@material/mwc-button": "^0.20.0",
        "@material/mwc-checkbox": "^0.20.0",
        "@material/mwc-dialog": "^0.20.0",
        "@material/mwc-formfield": "^0.20.0",
        "@material/mwc-icon": "^0.20.0",
        "@material/mwc-icon-button": "^0.20.0",
        "@material/mwc-list": "^0.20.0",
        "@material/mwc-menu": "^0.20.0",
        "@tarekraafat/autocomplete.js": "^8.2.1",
        "dotcms-models": "latest",
        "flatpickr": "^4.6.9"
    }
}
