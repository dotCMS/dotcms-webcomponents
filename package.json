{
    "name": "dotcms-webcomponents",
    "version": "0.0.12",
    "description": "DotCMS UI Web Components",
    "repository": {
        "type": "git",
        "url": "https://github.com/dotcms/dotcms-webcomponents"
    },
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/dotcms-webcomponents/dotcms-webcomponents.js",
    "files": [
        "dist/",
        "loader/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "storybook": "start-storybook -s dist -p 6006",
        "build-storybook": "build-storybook",
        "dev": "npm-run-all --parallel start storybook"
    },
    "devDependencies": {
        "@babel/core": "^7.9.0",
        "@stencil/core": "^1.16.4",
        "@stencil/sass": "^1.3.2",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-knobs": "^5.3.18",
        "@storybook/addon-notes": "^5.3.18",
        "@storybook/html": "^5.3.18",
        "@types/jest": "24.9.1",
        "@types/puppeteer": "1.20.3",
        "babel-loader": "^8.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "jest": "24.9.0",
        "jest-cli": "24.9.0",
        "npm-run-all": "^4.1.5",
        "puppeteer": "1.20.0",
        "write-file-webpack-plugin": "^4.5.1"
    },
    "license": "MIT",
    "dependencies": {
        "@material/mwc-button": "^0.17.2",
        "@material/mwc-checkbox": "^0.17.2",
        "@material/mwc-dialog": "^0.17.2",
        "@material/mwc-formfield": "^0.17.2",
        "@material/mwc-icon": "^0.17.2",
        "@material/mwc-icon-button": "^0.17.2",
        "@material/mwc-list": "^0.17.2",
        "@material/mwc-menu": "^0.17.2",
        "@tarekraafat/autocomplete.js": "^7.2.0",
        "dotcms-models": "0.0.8",
        "flatpickr": "^4.5.7"
    }
}
